<h1>Create a New Article<h1>

<%= render 'form'%>

<%= form_with scope: :article, url: articles_path, local: true do |f| %>
    <p>
        <%= f.label :title %><br/>
        <%= f.text_field :title %><br/>
    </p>
    <p>
        <%= f.label :description %><br/>
        <%= f.text_area :description %><br/>
    </p>
    <p>
        <%= f.submit %>
    </p>
<%end%>

<%= link_to 'Return to all articles', articles_path %>